{
    "manifest_version": 3,
    "name": "NandreMosaicHelper",
    "version": "1.0.0",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://mosaic.ludwig.gg/*"],
            "js": ["content.js"]
        }
    ]
}